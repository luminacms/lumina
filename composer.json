{
    "name": "xbhub/lumina",
    "type": "project",
    "description": "Lumina, The Last PHP Framework.",
    "keywords": [
        "framework",
        "lumina"
    ],
    "license": "MIT",
    "require": {
        "php": "^7.2.5",
        "brick/math": "^0.8.14",
        "endroid/qr-code": "^3.7",
        "fideloper/proxy": "^4.2",
        "fruitcake/laravel-cors": "^1.0",
        "guzzlehttp/guzzle": "^6.3",
        "laracasts/flash": "^3.1",
        "laravel/framework": "^7.3",
        "laravel/passport": "^8.4",
        "laravel/tinker": "^2.0",
        "laravel/ui": "^2.0",
        "league/commonmark": "^1.4",
        "maatwebsite/excel": "^3.1",
        "mews/captcha": "^3.1",
        "nwidart/laravel-modules": "^6.2",
        "rap2hpoutre/laravel-log-viewer": "^1.4",
        "spatie/laravel-permission": "^3.11",
        "spatie/laravel-responsecache": "6.2.*",
        "tymon/jwt-auth": "^1.0",
        "xbhub/laravel-filter": "dev-master",
        "xbhub/laravel-socialite": "^2.0"
    },
    "require-dev": {
        "xbhub/laravel-xgee": "dev-master",
        "facade/ignition": "^2.0",
        "fzaninotto/faker": "^1.9.1",
        "joshbrw/laravel-module-installer": "^0.1.4",
        "mockery/mockery": "^1.3.1",
        "nunomaduro/collision": "^4.1",
        "phpunit/phpunit": "^8.5"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "extra": {
        "module-dir": "modules",
        "laravel": {
            "dont-discover": []
        }
    },
    "autoload": {
        "psr-4": {
            "App\\": "app",
            "Modules\\": "modules/"
        },
        "classmap": [
            "database/seeds"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    },
    "repositories": {
        "packagist": {
            "type": "composer",
            "url": "https://mirrors.aliyun.com/composer/"
        }
    }
}
